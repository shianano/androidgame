    <TextView
        android:id="@+id/textView30S"
        android:layout_width="wrap_content"
        android:layout_height="24dp"
        android:text="@string/attack"
        android:textSize="18sp"
        app:layout_constraintBottom_toBottomOf="@+id/imageView16S"
        app:layout_constraintStart_toStartOf="@+id/MPBar2S"
        app:layout_constraintTop_toBottomOf="@+id/MPBar2S"
        app:layout_constraintVertical_bias="0.16000003" />

    <TextView
        android:id="@+id/MP2S"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:layout_constraintBottom_toTopOf="@+id/MPBar2S"
