    <ImageButton
        android:id="@+id/tate_button"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:background="@drawable/soubi_imagebutton"
        android:scaleType="fitCenter"
        android:src="@drawable/tate"
        app:layout_constraintEnd_toEndOf="@+id/buki_button"
        app:layout_constraintHeight_default="percent"
        app:layout_constraintHeight_percent="0.16"
        app:layout_constraintTop_toBottomOf="@+id/textView19S"
        app:layout_constraintWidth_default="percent"
        app:layout_constraintWidth_percent="0.3" />

    <ImageButton
        android:id="@+id/kabuto_button"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:background="@drawable/soubi_imagebutton"
        android:scaleType="fitCenter"
        android:src="@drawable/atama"
        app:layout_constraintEnd_toEndOf="@+id/tate_button"
        app:layout_constraintHeight_default="percent"
        app:layout_constraintHeight_percent="0.16"
        app:layout_constraintTop_toBottomOf="@+id/textView21S"
        app:layout_constraintWidth_default="percent"
        app:layout_constraintWidth_percent="0.3" />

    <ImageButton
        android:id="@+id/yoroi_button"
        android:layout_width="0dp"
