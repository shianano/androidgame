        tools:text="0" />

    <TextView
        android:id="@+id/textView20S"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/s"
        app:layout_constraintBottom_toTopOf="@+id/HPBar2S"
        app:layout_constraintEnd_toStartOf="@+id/maxHPS" />

    <ProgressBar
        android:id="@+id/HPBar2S"
