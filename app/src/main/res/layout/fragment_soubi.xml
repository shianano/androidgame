        android:layout_height="54dp"
        android:background="@drawable/soubi_list"
        android:elevation="2dp"
        android:spinnerMode="dialog"
        android:textAlignment="center"
        android:visibility="invisible"
        app:layout_constraintEnd_toStartOf="@+id/yoroi_button"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="@+id/yoroi_button" />

    <Spinner
        android:id="@+id/atama_list"
        android:layout_width="203dp"
        android:layout_height="54dp"
        android:layout_marginEnd="16dp"
        android:background="@drawable/soubi_list"
        android:elevation="2dp"
        android:spinnerMode="dialog"
        android:textAlignment="center"
        android:visibility="invisible"
        app:layout_constraintEnd_toStartOf="@+id/kabuto_button"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="@+id/kabuto_button" />

    <Spinner
        android:id="@+id/tate_list"
        android:layout_width="203dp"
        android:layout_height="54dp"
        android:layout_marginEnd="16dp"
        android:background="@drawable/soubi_list"
        android:elevation="2dp"
        android:spinnerMode="dialog"
        android:textAlignment="center"
        android:visibility="invisible"
        app:layout_constraintEnd_toStartOf="@+id/tate_button"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="@+id/tate_button" />

