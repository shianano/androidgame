        app:layout_constraintStart_toStartOf="@+id/MPBar2S" />

    <ProgressBar
        android:id="@+id/MPBar2S"
        style="?android:attr/progressBarStyleHorizontal"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:max="100"
        android:progress="100"
        android:progressDrawable="@drawable/custom_progress"
        app:layout_constraintBottom_toBottomOf="@+id/imageView16S"
        app:layout_constraintEnd_toEndOf="@+id/HPBar2S"
        app:layout_constraintHeight_default="percent"
        app:layout_constraintHeight_percent="0.02"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toStartOf="@+id/HPBar2S"
        app:layout_constraintTop_toBottomOf="@+id/HPBar2S"
        app:layout_constraintVertical_bias="0.22000003"
        app:layout_constraintWidth_default="percent"
        app:layout_constraintWidth_percent="0.35" />

    <TextView
        android:id="@+id/HP2S"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:layout_constraintBottom_toTopOf="@+id/HPBar2S"
        app:layout_constraintEnd_toStartOf="@+id/textView20S"
        tools:text="0" />

    <TextView
        android:id="@+id/maxHPS"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
