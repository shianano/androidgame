        app:layout_constraintTop_toTopOf="@+id/textView32S"
        tools:text="0" />

    <TextView
        android:id="@+id/defensePointS"
        android:layout_width="48dp"
        android:layout_height="20dp"
        android:textAlignment="viewEnd"
        android:textSize="18sp"
        app:layout_constraintEnd_toEndOf="@+id/attackPointS"
        app:layout_constraintTop_toTopOf="@+id/textView31S"
        tools:text="0" />

    <TextView
        android:id="@+id/attackPointS"
        android:layout_width="48dp"
        android:layout_height="20dp"
        android:textAlignment="viewEnd"
        android:textSize="18sp"
        app:layout_constraintEnd_toEndOf="@+id/MPBar2S"
        app:layout_constraintTop_toTopOf="@+id/textView30S"
        tools:text="0" />

    <TextView
        android:id="@+id/textView32S"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/MagicPower"
        android:textSize="18sp"
        app:layout_constraintBottom_toBottomOf="@+id/imageView16S"
        app:layout_constraintStart_toStartOf="@+id/textView31S"
        app:layout_constraintTop_toBottomOf="@+id/textView31S" />

    <TextView
        android:id="@+id/textView31S"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/defense"
