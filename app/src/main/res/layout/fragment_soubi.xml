        app:layout_constraintBottom_toTopOf="@+id/yoroi_button"
        app:layout_constraintEnd_toEndOf="@+id/yoroi_button"
        tools:text="よろい" />

    <Spinner
        android:id="@+id/buki_list"
        android:layout_width="203dp"
        android:layout_height="54dp"
        android:background="@drawable/soubi_list"
        android:elevation="2dp"
        android:spinnerMode="dialog"
        android:textAlignment="center"
        android:visibility="invisible"
        app:layout_constraintEnd_toStartOf="@+id/buki_button"
        app:layout_constraintStart_toStartOf="parent"
