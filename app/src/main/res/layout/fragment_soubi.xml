        app:layout_constraintEnd_toStartOf="@+id/buki_button"
        app:layout_constraintTop_toTopOf="@+id/imageView17S" />

    <TextView
        android:id="@+id/textView42S"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/level99"
        android:textSize="18sp"
        app:layout_constraintStart_toEndOf="@+id/level2S"
        app:layout_constraintTop_toTopOf="@+id/level2S" />

    <TextView
        android:id="@+id/level2S"
        android:layout_width="33dp"
        android:layout_height="24dp"
        android:textAlignment="viewEnd"
        android:textSize="18sp"
        app:layout_constraintBottom_toBottomOf="@+id/textView41S"
        app:layout_constraintStart_toEndOf="@+id/textView41S"
        app:layout_constraintTop_toTopOf="@+id/textView9"
        tools:text="0" />

    <TextView
        android:id="@+id/textView41S"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="8dp"
        android:text="@string/Level"
        android:textSize="18sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="@+id/imageView7"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="@+id/textView39S"
        app:layout_constraintTop_toBottomOf="@+id/textView39S"
        app:layout_constraintVertical_bias="0.0" />

    <TextView
        android:id="@+id/textView39S"
        android:layout_width="128dp"
        android:layout_height="32dp"
        android:text="@string/status"
        android:textSize="18sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintHeight_default="percent"
        app:layout_constraintHeight_percent="0.03"
        app:layout_constraintStart_toStartOf="@+id/HPBar2S"
        app:layout_constraintTop_toTopOf="@+id/imageView16S"
